{
  "manifest_version": 2,
  "content_scripts": [
    {
      "exclude_globs": [],
      "include_globs": ["*"],
      "js": ["index.js"],
      "css": ["main.css"],
      "matches": [
        "https://editor-mkii.morningstar.io/*",
        "https://editor-mkii-staging.morningstar.io/*"
      ],
      "run_at": "document_end"
    }
  ],
  "background": { "scripts": ["background.js"] },
  "converted_from_user_script": true,
  "description": "Power user features for the Morningstar web editor",
  "name": "Morningstar Editor Plus",
  "version": "1"
}
